{
  "name": "object-resolve-path",
  "version": "1.1.1",
  "description": "a simple utility function for getting a value at a path from an object with all the usecases properly covered",
  "main": "index.js",
  "scripts": {
    "dist": "npm run bundle && npm run minify",
    "minify": "browserify --standalone objectResolvePath --entry=./index.js ./index.js | uglifyjs -cm > object-resolve-path.min.js",
    "bundle": "browserify --standalone objectResolvePath --entry=./index.js ./index.js -o object-resolve-path.js",
    "test": "mocha"
  },
  "keywords": [
    "object",
    "property",
    "accessor",
    "string"
  ],
  "devDependencies": {
    "benchmark": "^2.1.1",
    "browserify": "^14.1.0",
    "chai": "^3.5.0",
    "lodash.get": "^4.4.2",
    "mocha": "^3.0.2",
    "uglifyjs": "^2.4.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/capaj/object-resolve-path.git"
  },
  "author": "capajj@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/capaj/object-resolve-path/issues"
  },
  "homepage": "https://github.com/capaj/object-resolve-path#readme"
}
